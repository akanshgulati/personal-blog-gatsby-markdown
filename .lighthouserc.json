{
  "ci": {
    "collect": {
      "numberOfRuns": 2,
      "url": [
        "http://localhost:8080/",
        "http://localhost:8080/tag/javascript",
        "http://localhost:8080/author/akansh",
        "http://localhost:8080/essential-tools-and-services-for-student-developer/"
      ]
    },
    "assert": {
      "preset": "lighthouse:no-pwa",
      "assertions": {
        "first-contentful-paint": [
          "error",
          {
            "maxNumericValue": 2500,
            "aggregationMethod": "optimistic"
          }
        ],
        "interactive": [
          "error",
          {
            "maxNumericValue": 5000,
            "aggregationMethod": "optimistic"
          }
        ],
        "uses-text-compression": "off",
        "uses-long-cache-ttl": "off",
        "uses-http2": "off"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
